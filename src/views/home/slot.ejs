        <script>
          // Get the tab list element
          const tabList = document.querySelector('.fun-tabs__tab-list');

          // Define the current position of the tab list
          let currentPosition = 0;

          // Function to slide the tab list to the left
          function slideLeft() {
            currentPosition += 100; // Adjust the value according to your design
            tabList.style.transform = `translateX(${currentPosition}px)`;
          }

          // Function to slide the tab list to the right
          function slideRight() {
            currentPosition -= 100; // Adjust the value according to your design
            tabList.style.transform = `translateX(${currentPosition}px)`;
          }
        </script>
        <script>

          let currentId;
          function openDialog(id) {
            
            openGameconfirmoverlay()
            currentId = id
            const gameDialog = document.getElementById('gameconfirm');
            gameDialog.style.display = 'block';

          }
          function openGameconfirmoverlay() {
            const gameDialog = document.getElementById('gameconfirmoverlay');
            gameDialog.style.display = 'block';

          }

          function closeDialog() {
            closeGameconfirmoverlay()
            currentId = null
            const gameDialog = document.getElementById('gameconfirm');
            gameDialog.style.display = 'none';

          }
          function closeGameconfirmoverlay() {
            const gameDialog = document.getElementById('gameconfirmoverlay');
            gameDialog.style.display = 'none';

          }

          function redirectJilliFunction() {

            const loadingImage = document.getElementById('loadingImage');
            loadingImage.style.display = 'block';
            if (currentId) {
              fetch(`/api/webapi/GetJilliboardGame/${currentId}`)
                .then(response => response.json())
                .then(response => {
                  closeDialog()
                  openGameconfirmoverlay()
                  const data = response?.data
                  if (data?.url) {
                    currentId = null
                    const newTab = window.open(data.url, '_blank');
                    const checkTabClosed = setInterval(() => {
                      if (newTab && newTab.closed) {
                        clearInterval(checkTabClosed);
                        closeDialog()
                        const loadingImage = document.getElementById('loadingImage');
                        loadingImage.style.display = 'none';
                        window.location.href = '/home';
                      }
                    }, 500);
                  }
                })
                .catch(error => {
                  const loadingImage = document.getElementById('loadingImage');
                  loadingImage.style.display = 'none';
                  console.error('Error:', error);
                });
            }

          }

          function redirectFunction(app_id) {
            fetch(`/api/webapi/GetTopBettingboardGame/${app_id}`)
              .then(response => response.json())
              .then(response => {
                const data = response?.data
                // window.open(data?.url, '_blank');
                const newTab = window.open(data?.url, '_blank');
                const checkTabClosed = setInterval(() => {
                  if (newTab && newTab.closed) {
                    clearInterval(checkTabClosed);
                    window.location.href = '/home#original';
                  }
                }, 500);
              })
              .catch(error => {
                console.error('Error:', error);
              });
          }


        </script>
