      const activeItemUpi = document.querySelector("#upi_list li.active");
      amount = parseInt(moneyInr.value);

      if (isNaN(amount)) {
        alertMess("Please enter a valid amount.");
      } else if (amount < 300 || amount > 100000) {
        alertMess("Amount must be between 300 and 100000 (inclusive).");
      } else if (!activeItemUpi) {
