                let isChecked = true;   
                function toggleCheckbox() {
                  const checkbox = document.querySelector('.van-checkbox');
                  const icon = checkbox.querySelector('.van-checkbox__icon');
                  const inrCurrency = document.getElementById('inrCurrency');
                  const usdtCurrency = document.getElementById('usdtCurrency');
