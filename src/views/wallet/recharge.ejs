            window.location.href = `/wallet/paynow/upi_autogateway?am=${amount}`;
            break;
          case PaymentMethodsMap.FFPAY:
            //handleFFPayGatewayPaymentRequest(amount);
           // handleManualUPIPaymentRequest(amount);
           window.location.href = `/wallet/paynow/upi_autogateway?am=${amount}`;
            break;
          case PaymentMethodsMap.UPI:
           // handleManualUPIPaymentRequest(amount);
           window.location.href = `/wallet/paynow/upi_autogateway?am=${amount}`;
            break;
          case PaymentMethodsMap.TYPAY:
            //handleSunpayGatewayPaymentRequest(amount);
            //handleManualUPIPaymentRequest(amount);
            window.location.href = `/wallet/paynow/upi_autogateway?am=${amount}`;
            break;
          default:
            alertMess("Select channel.");
            break;
        }
      }
      break;

    case "usdt_manual":
      amount = parseInt(moneyUsdt.value);
      if (isNaN(amount)) {
        alertMess("Please enter a valid amount.");
      } else if (amount < 5 || amount > 10000000000) {
        alertMess("Amount must be between 5 and 10000000000 (inclusive).");
      } else {
        handleUSDTPaymentRequest(amount);
      }
      break;

    case "manual_pay":
      amount = parseInt(moneyBank.value);
      if (isNaN(amount)) {
        alertMess("Please enter a valid amount.");
      } else if (amount < 300 || amount > 10000000000) {
        alertMess("Amount must be between 300 and 10000000000 (inclusive).");
      } else {
        handleBankPaymentRequest(amount);
      }
      break;

    default:
      amount = moneyInr.value;
      handleManualUPIPaymentRequest(amount);
      
      break;
  }
};

