                    icon.classList.add('van-checkbox__icon--checked');
                    usdtCurrency.style.display = 'block';
                    inrCurrency.style.display = 'none';
                    labelText.textContent = 'Convert INR';
                     
                  } else {
                    icon.classList.remove('van-checkbox__icon--checked');
                    inrCurrency.style.display = 'block';
                    usdtCurrency.style.display = 'none';
