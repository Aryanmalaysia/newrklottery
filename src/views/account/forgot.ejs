        document.addEventListener('DOMContentLoaded', function () {
            const dropdown = document.querySelector('.dropdown');
            const dropdownList = dropdown.querySelector('.dropdown__list');

            dropdown.addEventListener('click', function () {
                dropdownList.classList.toggle('active');
            });
 
            document.addEventListener('click', function (event) {
                if (!dropdown.contains(event.target)) {
                    dropdownList.classList.remove('active');
                }
            });
        });

        const dropdownValue = document.querySelector('.dropdown__value');
        const dropdownListItems = document.querySelectorAll('.dropdown__list-item');

        function changeCountry(countryCode) {
            dropdownValue.querySelector('span').textContent = '+' + countryCode;

            dropdownListItems.forEach(item => {
                item.classList.remove('active');
            });

            // Find and add 'active' class to the item with matching countryCode
            dropdownListItems.forEach(item => {
                if (item.textContent.includes('+' + countryCode)) {
                    item.classList.add('active');
                }
            });
        }
 
 
    </script>
