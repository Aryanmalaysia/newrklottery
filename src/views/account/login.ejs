        const checkboxes = document.querySelectorAll('.van-checkbox');

// Add click event listener to each checkbox
checkboxes.forEach(checkbox => {
  checkbox.addEventListener('click', function() {
    // Toggle the aria-checked attribute
    const isChecked = this.getAttribute('aria-checked') === 'true';
    this.setAttribute('aria-checked', !isChecked);
    
    // Toggle the checked/unchecked visual state
    const icon = this.querySelector('.van-checkbox__icon');
    if (isChecked) {
      icon.classList.remove('van-checkbox__icon--checked');
    } else {
      icon.classList.add('van-checkbox__icon--checked');
    }
  });
});


 const dropdownValue = document.querySelector('.dropdown__value');
const dropdownListItems = document.querySelectorAll('.dropdown__list-item');

 function changeCountry(countryCode) {
     dropdownValue.querySelector('span').textContent = '+' + countryCode;

     dropdownListItems.forEach(item => {
        item.classList.remove('active');
    });

    // Find and add 'active' class to the item with matching countryCode
    dropdownListItems.forEach(item => {
        if (item.textContent.includes('+' + countryCode)) {
            item.classList.add('active');
        }
    }); 
}
 
 

